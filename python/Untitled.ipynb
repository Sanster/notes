   ],
   "source": [
    "import difflib\n",
    "\n",
    "a = '社456会保障号码'\n",
    "b = '社会123保证号码'\n",
    "eq = difflib.SequenceMatcher()\n",
    "eq.set_seqs(a, b)\n",
    "print(eq.ratio())\n",
    "\n",
    "for m in eq.get_matching_blocks()[:-1]:\n",
    "    print(m)\n",
    "    print(f\"a: {a[m.a:m.a + m.size]}\")\n",
    "    print(f\"b: {b[m.b:m.b + m.size]}\")"
   ]
  }
 ],
